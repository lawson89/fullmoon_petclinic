{% function block.content() %}
  <h2>
    New Pet
  </h2>
  <form class="form-horizontal" method="post">
     {% render('fragments/form', {form = form}) %}
    
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
